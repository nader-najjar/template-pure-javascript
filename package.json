{
  "name": "template-pure-javascript",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=24 <25",
    "pnpm": ">=10 <11"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist && printf '\\033[0;32mBUILD SUCCEEDED\\033[0m\n'",
    "build": "pnpm install --frozen-lockfile && pnpm run check && tsc && pnpm run test && printf '\\033[0;32mBUILD SUCCEEDED\\033[0m\n'",
    "check": "pnpm run lint",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
